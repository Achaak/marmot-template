{
  "name": "@pikas-template/storybook",
  "version": "1.0.0",
  "scripts": {
    "build": "build-storybook",
    "dev": "start-storybook -p 6006",
    "format": "prettier --cache --write \"**/*.{ts,tsx,mdx}\"",
    "lint": "cross-env TIMING=1 eslint \"./**/*.{ts,tsx}\" --fix",
    "start": "npx http-server ./storybook-static -p 3000",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf storybook-static"
  },
  "dependencies": {
    "@pikas-template/ui": "workspace:*",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "@pikas-template/prettier": "workspace:*",
    "@pikas-template/tsconfig": "workspace:*",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/addons": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/cli": "^6.4.21",
    "@storybook/components": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/react": "^6.4.21",
    "@storybook/theming": "^6.4.21",
    "@types/node": "^18.7.15",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "cross-env": "^7.0.3",
    "eslint": "8.23.1",
    "http-server": "^14.1.1",
    "prettier": "2.7.1",
    "typescript": "4.8.3",
    "webpack": "^5.73.0"
  }
}
