{
  "name": "@pikas-template/ui",
  "version": "0.0.0",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --cache --write \"**/*.{ts,tsx,.md}\"",
    "lint": "cross-env TIMING=1 eslint src --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "license": "MIT",
  "dependencies": {
    "@pikas-ui/button": "^3.0.11",
    "@pikas-ui/icons": "^3.0.8",
    "@pikas-ui/loader": "^3.0.8",
    "@pikas-ui/styles": "^3.0.8",
    "@pikas-ui/text": "^4.0.8",
    "@pikas-ui/textfield": "^3.0.9",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pikas-template/prettier": "workspace:*",
    "@pikas-template/tsconfig": "workspace:*",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.7.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  }
}
