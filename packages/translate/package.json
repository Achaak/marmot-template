    "@types/node": "^18.0.1",
    "@types/react": "^18.0.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  },
  "node": "12.20.0 || 14.13.1 || >=16.0.0"
